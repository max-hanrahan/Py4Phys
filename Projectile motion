    "# this loop is how we draw sol's for both curves\n",
    "sols_list = [1,2]\n",
    "vel = 9\n",
    "\n",
    "for a in sols_list:\n",
    "    draw_trajectory(vel, a)\n",
    "    \n",
    "plt.legend(['solution1', 'solution2'])"
   ]
